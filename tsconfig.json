{
    "compilerOptions": {
        "target": "ES5",
        "lib": ["dom", "dom.iterable", "esnext", "ES2022"],
        "allowJs": true,
        "sourceMap": false,
        "jsx": "react",
        "strict": false,
        "isolatedModules": false,
        "esModuleInterop": true,
        "noImplicitReturns": true,
        "noImplicitAny": false,
        "baseUrl": "./resources",
        "paths": {
            "@/*": ["ts/*"]
        }
    }
}
